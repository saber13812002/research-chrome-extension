{
  "name": "research tools extension",
  "version": "1.0",
  "description": "",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["bookmarks", "storage"],
  "content_scripts": [{
    "matches": [
      "https://*.aparat.com/*",
      "https://*.google.com/*",
      "https://*.stackoverflow.com/*",
      "https://*.youtube.com/*"
    ],
    "js": [
      "aparat.js",
      "stackoverflow.js",
      "youtube.js",
      "google.js"
    ]
  }]
}